
public class Einmaleins {

	public static void main(String[] args) {
		for(int eins = 1; eins <= 10; eins++) {
			int i = 1;
			i = i * eins;
			System.out.print(i + " ");
			if(eins == 10) {
			System.out.println("");
			}
		}
		for(int zwei = 2; zwei <= 20; zwei++) {
			int i = 1;
			i = i * zwei;
			System.out.print(i + " ");
			if(zwei == 10) {
			System.out.println("");
			}
		}
		
	}

}
